{
  "Code": {
    "theme": "solarized_light",
    "displayLineNumber": false
  },
  "Editor": {
    "theme": "neat",
    "displayLineNumber": true,
    "vimKeyBinding": true,
    "autoPairCharacters": true,
    "tabSize": 4,
    "indentUnit": 4,
    "indentWithTabs": true,
    "displayActiveLine": true,
    "fontSize": 13,
    "userTheme": "default"
  },
  "General": {
    "enableSyncScroll": true,
    "playKeypressSound": false,
    "enableAutoComplete": true,
    "enableLastFileRestore": true,
    "displayLanguage": "ja",
    "dateFormat": "LLL"
  },
  "Markdown": {
    "tables": true,
    "breaks": true,
    "sanitize": false,
    "smartLists": true,
    "smartypants": true,
    "mathjax": false,
    "dollarSign": false
  },
  "Viewer": {
    "theme": "solarized-light",
    "clickableLink": true,
    "fontSize": 15,
    "userTheme": "default"
  },
  "_version": "0.11.1"
}